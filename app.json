{
    "apiVersion": "apps.openshift.io/v1",
    "kind": "DeploymentConfig",
    "metadata": {
        "creationTimestamp": null,
        "generation": 1,
        "labels": {
            "app": "${APP_NAME}"
        },
        "name": "${APP_NAME}"
    },
    "spec": {
        "replicas": 1,
        "revisionHistoryLimit": 10,
        "selector": {
            "app": "${APP_NAME}",
            "deploymentconfig": "${APP_NAME}"
        },
        "strategy": {
            "activeDeadlineSeconds": 21600,
            "resources": {},
            "rollingParams": {
                "intervalSeconds": 1,
                "maxSurge": "25%",
                "maxUnavailable": "25%",
                "timeoutSeconds": 600,
                "updatePeriodSeconds": 1
            },
            "type": "Rolling"
        },
        "template": {
            "metadata": {
                "annotations": {
                    "openshift.io/generated-by": "OpenShiftNewApp"
                },
                "creationTimestamp": null,
                "labels": {
                    "app": "${APP_NAME}",
                    "deploymentconfig": "${APP_NAME}"
                }
            },
            "spec": {
                "containers": [
                    {
                        "env": [
                            {
                                "name": "APP_CONFIG",
                                "value": "${APP_CONFIG}"
                            },
                            {
                                "name": "APP_MODULE",
                                "value": "${APP_MODULE}"
                            },
                            {
                                "name": "MYSQL_DB",
                                "value": "${MYSQL_DB}"
                            },
                            {
                                "name": "MYSQL_NAME",
                                "value": "${MYSQL_NAME}"
                            },
                            {
                                "name": "MYSQL_PASSWORD",
                                "valueFrom": {
                                    "secretKeyRef": {
                                        "key": "MYSQL_PASSWORD",
                                        "name": "my-secret"
                                    }
                                }
                            },
                            {
                                "name": "MYSQL_USER",
                                "valueFrom": {
                                    "secretKeyRef": {
                                        "key": "MYSQL_USER",
                                        "name": "my-secret"
                                    }
                                }
                            }
                        ],
                        "image": "image-registry.openshift-image-registry.svc:5000/${DEV_PROJECT}/${APP_NAME}:latest",
                        "imagePullPolicy": "Always",
                        "name": "${APP_NAME}",
                        "ports": [
                            {
                                "containerPort": 8080,
                                "protocol": "TCP"
                            }
                        ],
                        "resources": {},
                        "terminationMessagePath": "/dev/termination-log",
                        "terminationMessagePolicy": "File"
                    }
                ],
                "dnsPolicy": "ClusterFirst",
                "restartPolicy": "Always",
                "schedulerName": "default-scheduler",
                "securityContext": {},
                "terminationGracePeriodSeconds": 30
            }
        },
        "test": false,
        "triggers": [
            {
                "type": "ConfigChange"
            },
            {
                "imageChangeParams": {
                    "automatic": true,
                    "containerNames": [
                        "${APP_NAME}"
                    ],
                    "from": {
                        "kind": "ImageStreamTag",
                        "name": "${APP_NAME}:latest",
                        "namespace": "${DEV_PROJECT}"
                    }
                },
                "type": "ImageChange"
            }
        ]
    },
    "status": {
        "availableReplicas": 0,
        "latestVersion": 0,
        "observedGeneration": 0,
        "replicas": 0,
        "unavailableReplicas": 0,
        "updatedReplicas": 0
    }
}
